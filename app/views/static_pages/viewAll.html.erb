<ul>
  <h1>Users</h1>
  <h3>All passwords are @@pw@@</h3>
    <% @users.each do |user| %>
      <li>
          <%= user.username %>
      </li>
    <% end %>
</ul>

<ul>
  <h1>Teams</h1>
    <% @teams.each do |team| %>
      <li>
          <ul>
            <li>Name: <%= team.name %></li>
            <li>Creator: <%= team.creator.username %></li>
            <li>Invited Users:
              <ul>
                <% team.invited_users.each do |user| %>
                  <li><%= user.username %></li>
                <% end %>
              </ul>
            </li>
          </ul>
      </li>
    <% end %>
</ul>
